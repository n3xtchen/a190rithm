{
  "mcpServers": {
    "jupyter": {
      "command": "uvx",
      "args": ["jupyter-mcp-server@latest"],
      "env": {
        "JUPYTER_URL": "http://${JUPYTER_HOST:-127.0.0.1}:${JUPYTER_PORT:-8888}",
        "JUPYTER_TOKEN": "${JUPYTER_TOKEN:-MY_TOKEN}",
        "ALLOW_IMG_OUTPUT": "true"
      }
    }
  }
}
